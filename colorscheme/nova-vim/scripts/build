#!/bin/bash
set -euo pipefail

warning='" WARNING: Do not edit this file directly - it is built from the src directory'
echo $warning > colors/nova.vim
echo $warning > autoload/airline/themes/nova.vim
echo $warning > autoload/lightline/colorscheme/nova.vim
node src/index.js >> colors/nova.vim
node src/airline.js >> autoload/airline/themes/nova.vim
node src/lightline.js >> autoload/lightline/colorscheme/nova.vim
